{
  "name": "@crazyorr/loan",
  "version": "1.1.0",
  "description": "Interest calculation of loans",
  "main": "./dist/loan.js",
  "module": "./dist/loan.mjs",
  "types": "./dist/loan.d.ts",
  "exports": {
    "types": "./dist/loan.d.ts",
    "import": "./dist/loan.mjs",
    "default": "./dist/loan.js"
  },
  "files": [
    "dist/"
