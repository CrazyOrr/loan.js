    "@vitest/coverage-v8": "^4.0.13",
    "cspell": "^9.3.2",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.10"
  }
